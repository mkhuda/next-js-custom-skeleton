{
  "name": "next-js-skeleton",
  "version": "1.0.0",
  "description": "NextJS Skeleton",
  "main": "index.js",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 next build",
    "app": "node server",
    "build": "next build",
    "dev": "next",
    "eslint": "./node_modules/.bin/eslint pages/ components/ libs/",
    "export": "next export",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "production": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "react",
    "nextjs"
  ],
  "author": "Muhammad K. Huda",
  "license": "ISC",
