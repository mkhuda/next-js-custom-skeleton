      "^layouts(.*)$": "<rootDir>/layouts",
      "^root(.*)$": "<rootDir>/root"
    },
    "testPathIgnorePatterns": [
      "./node_modules/"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "busyman": "^0.3.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "dayjs": "^1.7.4",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "frisbee": "^2.0.5",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.1.1",
    "next-redux-saga": "^3.0.0-beta.1",
    "next-redux-wrapper": "^2.0.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "redux-devtools-extension": "^2.13.5"
